{
  "name": "dari-backend-v2-deployment",
  "version": "1.0.0",
  "description": "Node.js deployment scripts for DARI V2 Backend",
  "main": "deploy.js",
  "scripts": {
    "deploy:dev": "node deploy.js dev",
    "deploy:prod": "node deploy.js prod",
    "deploy:stop": "node deploy.js stop",
    "deploy:logs": "node deploy.js logs",
    "deploy:status": "node deploy.js status",
    "deploy:migrate": "node deploy.js migrate",
    "deploy:backup": "node deploy.js backup",
    "deploy:clean": "node deploy.js clean",
    "docker:build": "docker build -t dari-backend-v2 .",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all",
    "health:check": "node scripts/health-check.js",
    "env:validate": "node scripts/validate-env.js",
    "setup": "npm install && node scripts/setup.js"
  },
  "keywords": [
    "dari",
    "fastapi",
    "deployment",
    "docker",
    "postgresql"
  ],
  "author": "DARI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "redis-server": "^1.2.2",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
